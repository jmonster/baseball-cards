{
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build"
    }
  ],
  "name": "dealzilla-spa",
  "routes": [
    {
      "src": "^/assets/(.*)",
      "dest": "/assets/$1",
      "headers": { "cache-control": "s-maxage=60" }
    },
    {
      "src": "^/robots.txt",
      "dest": "/robots.txt",
      "headers": { "cache-control": "s-maxage=3600" }
    },
    {
      "src": "^/(.*)",
      "dest": "/index.html",
      "headers": { "cache-control": "s-maxage=30" }
    }
  ],
  "version": 2,
  "alias": "dealzil.la"
}
