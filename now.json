{
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build"
    }
  ],
  "name": "dealzilla-spa",
  "routes": [
    {
      "src": "^/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "^/robots.txt",
      "dest": "/robots.txt"
    },
    {
      "src": "^/(.*)",
      "dest": "/index.html"
    }
  ],
  "version": 2
}
